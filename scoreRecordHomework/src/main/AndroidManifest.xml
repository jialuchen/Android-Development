<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jialu.cmu.edu.score.record">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:name="jialu.cmu.edu.score.record.ui.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="jialu.cmu.edu.score.record.ui.AddActivity"
            android:label="@string/add" />

        <activity
            android:name="jialu.cmu.edu.score.record.ui.DeleteActivity"
            android:label="@string/delete" />

        <activity
            android:name="jialu.cmu.edu.score.record.ui.StatisticsActivity"
            android:label="@string/statistic" />
    </application>

</manifest>
